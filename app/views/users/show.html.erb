<h2><%= @user.email %> has created these events </h2>
<ul>
  <% @user.created_events.each do |event| %>
    <li><%= link_to event.title, event_path(event) %></li>
  <% end %>
</ul>

<h2><%= @user.email %> is attending these events </h2>

<ul>
  <% @user.attended_events.each do |event| %>
    <li><%= link_to event.title, event_path(event) %></li>
  <% end %>
</ul>
